<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.god.kahit">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/kahit_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/kahit_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".view.lotteryItemView"></activity>
        <activity
            android:name=".view.CategoryView"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"></activity>
        <activity
            android:name=".view.LotteryView"
            android:exported="true" />
        <activity
            android:name=".view.MainActivityView"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.ChooseGameView"
            android:parentActivityName=".view.MainActivityView"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.god.kahit.view.ChooseGameView" />
        </activity>
        <activity
            android:name=".view.HostCreateRoomView"
            android:parentActivityName=".view.ChooseGameView"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.god.kahit.view.HostCreateRoomView" />
        </activity>
        <activity
            android:name=".view.HotSwapAddPlayersView"
            android:parentActivityName=".view.HotSwapGameModeView"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.god.kahit.view.HotSwapGameModeView" />
        </activity>
        <activity
            android:name=".view.HotSwapGameModeView"
            android:parentActivityName=".view.ChooseGameView">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.god.kahit.view.ChooseGameView" />
        </activity>
        <activity

            android:name=".view.JoinRoomView"
            android:parentActivityName=".view.ChooseGameView"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.god.kahit.view.HostCreateRoomView" />
        </activity>
        <activity
            android:name=".view.QuestionView"
            android:parentActivityName=".view.MainActivityView"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.god.kahit.view.ChooseGameView" />
        </activity>
        <activity
            android:name=".view.ScorePageView"
            android:parentActivityName=".view.MainActivityView"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.god.kahit.view.ChooseGameView" />
        </activity>
        <activity
            android:name=".view.TeamArrangementView"
            android:parentActivityName=".view.MainActivityView"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.god.kahit.view.ChooseGameView" />
        </activity>
        <activity
            android:name=".view.StoreView"
            android:parentActivityName=".view.MainActivityView">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.god.kahit.view.ChooseGameView" />
        </activity>
        <activity
            android:name=".view.AfterQuestionScorePageView"
            android:parentActivityName=".view.QuestionView"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.god.kahit.view.QuestionView" />
        </activity>
    </application>
</manifest>