<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.DrawerLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.MainActivityClass"
    android:id="@+id/Drawer_layout"
    android:fitsSystemWindows="true">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/textView10"
            android:layout_width="138dp"
            android:layout_height="94dp"
            android:layout_marginTop="252dp"
            android:layout_marginEnd="200dp"
            android:text="swipe from right to open store"
            android:textSize="24sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </android.support.constraint.ConstraintLayout>

    <android.support.design.widget.NavigationView
        android:id="@+id/navigationView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="end">

        <android.support.constraint.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/pointsText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="100dp"
                android:text="TextView"
                android:textSize="30sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/itemType1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="200dp"
                android:text="TextView"
                android:textSize="24sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/itemType2"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="356dp"
                android:text="TextView"
                android:textSize="24sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/itemType3"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="512dp"
                android:text="TextView"
                android:textSize="24sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageButton
                android:id="@+id/itemIcon"
                android:layout_width="64dp"
                android:layout_height="64dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="260dp"
                android:contentDescription="@string/item_icon"
                android:src="@drawable/strong"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageButton
                android:id="@+id/itemIcon3"
                android:layout_width="64dp"
                android:layout_height="64dp"
                android:layout_marginStart="212dp"
                android:layout_marginTop="260dp"
                android:contentDescription="@string/item_icon"
                android:src="@drawable/strong"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageButton
                android:id="@+id/itemIcon4"
                android:layout_width="64dp"
                android:layout_height="64dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="420dp"
                android:contentDescription="@string/item_icon"
                android:src="@drawable/strong"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageButton
                android:id="@+id/itemIcon5"
                android:layout_width="64dp"
                android:layout_height="64dp"
                android:layout_marginStart="116dp"
                android:layout_marginTop="420dp"
                android:contentDescription="@string/item_icon"
                android:src="@drawable/strong"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageButton
                android:id="@+id/itemIcon6"
                android:layout_width="64dp"
                android:layout_height="64dp"
                android:layout_marginStart="212dp"
                android:layout_marginTop="420dp"
                android:contentDescription="@string/item_icon"
                android:src="@drawable/strong"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageButton
                android:id="@+id/itemIcon7"
                android:layout_width="64dp"
                android:layout_height="64dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="580dp"
                android:contentDescription="@string/item_icon"
                android:src="@drawable/strong"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageButton
                android:id="@+id/itemIcon8"
                android:layout_width="64dp"
                android:layout_height="64dp"
                android:layout_marginStart="116dp"
                android:layout_marginTop="580dp"
                android:contentDescription="@string/item_icon"
                android:src="@drawable/strong"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageButton
                android:id="@+id/itemIcon9"
                android:layout_width="64dp"
                android:layout_height="64dp"
                android:layout_marginStart="212dp"
                android:layout_marginTop="580dp"
                android:contentDescription="@string/item_icon"
                android:src="@drawable/strong"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageButton
                android:id="@+id/itemIcon2"
                android:layout_width="64dp"
                android:layout_height="64dp"
                android:layout_marginStart="116dp"
                android:layout_marginTop="260dp"
                android:contentDescription="@string/item_icon"
                android:src="@drawable/strong"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        </android.support.constraint.ConstraintLayout>

    </android.support.design.widget.NavigationView>
</android.support.v4.widget.DrawerLayout>